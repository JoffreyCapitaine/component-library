{
  "name": "lc-component-library",
  "version": "1.6.4",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/lc-component-library.umd.js",
  "module": "./dist/lc-component-library.es.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "build": "vite build",
    "storybook": "start-storybook -p 6006",
    "dev": "vite",
    "build-storybook": "build-storybook",
    "test:unit": "jest --watch",
    "test:prod": "node ./node_modules/.bin/jest"
  },
  "exports": {
    ".": {
      "import": "./dist/lc-component-library.es.js",
      "require": "./dist/lc-component-library.umd.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "esno": "0.13.0",
    "vue": "3.2.26"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.15.0",
    "@babel/core": "^7.16.7",
    "@storybook/addon-a11y": "^6.4.10",
    "@storybook/addon-actions": "^6.4.10",
    "@storybook/addon-essentials": "^6.4.10",
    "@storybook/addon-links": "^6.4.10",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3": "^6.4.10",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@vitejs/plugin-vue": "2.0.1",
    "@vue/test-utils": "^2.0.0-rc.6",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "chromatic": "6.3.4",
    "cross-env": "^7.0.3",
    "eslint": "8.6.0",
    "fast-glob": "^3.2.10",
    "jest": "^26.6.3",
    "postcss": "^8.4.5",
    "postcss-loader": "^4.1.0",
    "tailwindcss": "^3.0.12",
    "ts-jest": "^26.5.6",
    "tsup": "^5.11.11",
    "typescript": "4.5.4",
    "vite": "2.7.10",
    "vue-jest": "vue-jest@5",
    "vue-loader": "^16.2.0",
    "vue-tsc": "0.30.2"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config",
    "ignorePatterns": [
      "build/**/*.js"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}
